{
  "name": "@jupiterone/graph-qualys",
  "version": "0.1.0",
  "description": "Qualys integration for JupiterOne",
  "license": "MPL-2.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "j1-integration collect",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
